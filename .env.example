# Database (Supabase)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-role-key
DATABASE_URL=postgresql://postgres:password@db.your-project.supabase.co:5432/postgres

# Anthropic (Claude API)
ANTHROPIC_API_KEY=sk-ant-...

# OpenAI (API)
OPENAI_API_KEY=sk-...
OPENAI_MODEL=gpt-4o-mini
LLM_PROVIDER=anthropic  # anthropic | openai | auto

# External APIs
EVENTBRITE_API_KEY=your-eventbrite-key
MEETUP_API_KEY=your-meetup-key
GOLDEN_VOLUNTEER_API_KEY=your-golden-volunteer-key

# Application
ENV=development
LOG_LEVEL=INFO

# Sentry (Error Tracking)
NEXT_PUBLIC_SENTRY_DSN=  # Get from sentry.io project settings
